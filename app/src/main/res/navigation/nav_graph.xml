<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="uz.ravshanbaxranov.a4pics1word.ui.screen.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_playgroundFragment"
            app:destination="@id/playgroundFragment" />
    </fragment>
    <fragment
        android:id="@+id/playgroundFragment"
        android:name="uz.ravshanbaxranov.a4pics1word.ui.screen.PlaygroundFragment"
        android:label="fragment_playground"
        tools:layout="@layout/fragment_playground">
        <argument
            android:name="playground"
            app:argType="string" />
    </fragment>
</navigation>